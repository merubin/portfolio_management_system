<h2>USER LIST: Current User=:<%=@current_user.name%></h2>
<h2><%= link_to "New User",new_user_path %><h2>
<ul>
  <% @users.each do |user| %>
    <li><%= link_to user.name, user %></li>
    <img src="<%=user.avatar_url%>" alt=<%= user.name %> height="200" width=auto>
    <%= link_to "Delete User #{user.name}",user_path(user), method: :delete %>
  <% end %>
</ul>
